commands:
  01_download_chrome:
    command: "wget https://dl.google.com/linux/direct/google-chrome-stable_current_x86_64.rpm"
    ignoreErrors: true
  02_install_chrome:
    command: "sudo dnf install -y ./google-chrome-stable_current_x86_64.rpm"
    ignoreErrors: true
  03_cleanup:
    command: "rm -f google-chrome-stable_current_x86_64.rpm"
    ignoreErrors: true